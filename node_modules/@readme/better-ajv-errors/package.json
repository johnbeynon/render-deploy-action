{
  "name": "@readme/better-ajv-errors",
  "version": "1.4.4",
  "description": "JSON Schema validation for Human",
  "license": "Apache-2.0",
  "author": "Rifat Nabi <to.rifat@gmail.com>",
  "main": "lib/index.js",
  "typings": "typings.d.ts",
  "files": [
    "lib",
    "index.js",
    "typings.d.ts"
  ],
  "engines": {
    "node": "^12 || ^14 || ^16"
  },
  "keywords": [
    "json-schema",
    "ajv",
    "ajv-errors"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/better-ajv-errors.git"
  },
  "bugs": {
    "url": "https://github.com/readmeio/better-ajv-errors/issues"
  },
  "scripts": {
    "build": "babel src -d lib",
    "lint": "eslint .",
    "prebuild": "rm -rf lib",
    "prepack": "npm run build",
    "prepare": "husky install",
    "prerelease": "npm run build",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.js\"",
    "test": "jest --coverage",
    "test-ci": "jest --coverage --colors"
  },
  "dependencies": {
    "@babel/code-frame": "^7.16.0",
    "@babel/runtime": "^7.17.7",
    "@humanwhocodes/momoa": "^2.0.3",
    "chalk": "^4.1.2",
    "json-to-ast": "^2.0.3",
    "jsonpointer": "^5.0.0",
    "leven": "^3.1.0"
  },
  "devDependencies": {
    "@apidevtools/openapi-schemas": "^2.1.0",
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.0",
    "@readme/eslint-config": "^8.5.1",
    "ajv": "^8.10.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^8.11.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-fixtures": "^0.6.0",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.1"
  },
  "peerDependencies": {
    "ajv": "4.11.8 - 8"
  },
  "prettier": "@readme/eslint-config/prettier",
  "jest": {
    "coveragePathIgnorePatterns": [
      "lib/"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.test.js"
    ]
  }
}
